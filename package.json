{
  "name": "resume",
  "homepage": "https://idin-khayami.github.io/resume",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "date-fns": "^1.30.1",
    "lint-staged": "^7.3.0",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-icons": "^2.2.7",
    "react-scripts": "^5.0.1",
    "stringstream": "^1.0.0",
    "styled-components": "^3.4.10"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "format": "prettier --single-quote --write src/**/*.{js,jsx,json,css}",
    "precommit": "lint-staged",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "gh-pages": "^1.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
